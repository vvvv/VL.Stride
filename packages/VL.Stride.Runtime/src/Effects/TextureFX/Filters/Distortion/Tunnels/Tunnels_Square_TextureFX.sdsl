shader Tunnels_Square_TextureFX : Tunnels
{
	stage float3 Tunnel(float2 p)
	{
		float2 pRot = r2d(p, Rotate);
		float2 uv;
		float r = pow(pow(pRot.x*pRot.x,3) + pow(pRot.y*pRot.y,3), 1.0/(Dist*8));
		uv.x = .5*time + 0.5/r;
		uv.y = (atan2(pRot.y,pRot.x)/3.1416+Offset);
		return float3(uv, r);
	}
};