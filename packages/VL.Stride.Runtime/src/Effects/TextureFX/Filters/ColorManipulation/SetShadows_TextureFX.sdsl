[Category("Filter")]
[Summary("Set the shadows of a texture")]
[Tags("Color Grading")]

shader SetShadows_TextureFX : FilterBase, ColorGradingOperations
{
    float4 Filter(float4 tex0col)
    {
        float4 tex = Texture1.SampleLevel(LinearSampler, streams.TexCoord, 0);
        return SetShadows(tex0col, tex);
    }
};