[Category("Filter")]
[Summary("Set the Midtones of a texture")]
[Tags("Color Grading")]

shader SetMidtones_TextureFX : FilterBase, ColorGradingOperations
{
    float4 Filter(float4 tex0col)
    {
        float4 tex = Texture1.SampleLevel(LinearSampler, streams.TexCoord, 0);
        return SetMidtones(tex0col, tex);
    }
};